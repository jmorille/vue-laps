{
  "name": "vue-laps",
  "version": "0.9.4-SNAPSHOT",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "test": "run-p type-check lint",
    "preview": "vite preview",
    "test:unit": "vitest  --dir src/tests  --root src/vuejs",
    "test:coverage": "vitest --coverage --dir src/tests --root src/vuejs",
    "sbom": "cdxgen -r --required-only -p -o target/bom.json",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint  --config eslint.config.mjs --fix .",
    "lint-old": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/vuejs"
  },
  "dependencies": {
    "@braintree/sanitize-url": "7.1.1",
    "@dsb-norge/vue-keycloak-js": "3.0.2",
    "@eslint/eslintrc": "3.3.0",
    "@mdi/font": "7.4.47",
    "@vueuse/core": "12.8.2",
    "axios": "1.8.1",
    "dayjs": "1.11.13",
    "globals": "16.0.0",
    "jwt-decode": "4.0.0",
    "keycloak-js": "26.1.3",
    "loglevel": "1.9.2",
    "loglevel-plugin-prefix": "0.8.4",
    "pinia": "3.0.1",
    "typescript-eslint": "8.26.0",
    "vue": "3.5.13",
    "vue-i18n": "11.1.1",
    "vue-router": "4.5.0",
    "vuetify": "3.7.15"
  },
  "devDependencies": {
    "@cyclonedx/cdxgen": "11.2.1",
    "@intlify/unplugin-vue-i18n": "6.0.3",
    "@rushstack/eslint-patch": "1.10.5",
    "@testing-library/vue": "8.1.0",
    "@tsconfig/node20": "20.1.4",
    "@types/jsdom": "21.1.7",
    "@types/node": "20.14.11",
    "@vitejs/plugin-vue": "5.2.1",
    "@vitest/coverage-v8": "3.0.7",
    "@vue/eslint-config-prettier": "10.2.0",
    "@vue/eslint-config-typescript": "14.5.0",
    "@vue/test-utils": "2.4.6",
    "@vue/tsconfig": "0.7.0",
    "concurrently": "9.1.2",
    "eslint": "9.21.0",
    "eslint-plugin-vue": "10.0.0",
    "eslint-plugin-vuetify": "2.5.1",
    "jsdom": "26.0.0",
    "material-design-icons-iconfont": "6.7.0",
    "npm-run-all": "4.1.5",
    "prettier": "3.5.3",
    "resize-observer-polyfill": "1.5.1",
    "typescript": "5.8.2",
    "vite": "6.2.0",
    "vite-plugin-pwa": "0.21.1",
    "vite-plugin-vuetify": "2.1.0",
    "vitest": "3.0.7",
    "vitest-sonar-reporter": "2.0.0",
    "vue-cli-plugin-i18n": "2.3.2",
    "vue-tsc": "2.2.8"
  }
}
