{
  "name": "vue-laps",
  "version": "0.9.4-SNAPSHOT",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "test": "run-p type-check lint",
    "preview": "vite preview",
    "test:unit": "vitest  --dir src/tests  --root src/vuejs",
    "test:coverage": "vitest --coverage --dir src/tests --root src/vuejs",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/vuejs"
  },
  "dependencies": {
    "@braintree/sanitize-url": "7.0.0",
    "@dsb-norge/vue-keycloak-js": "2.4.0",
    "@mdi/font": "7.4.47",
    "@vueuse/core": "10.9.0",
    "axios": "1.6.7",
    "dayjs": "1.11.10",
    "file-saver": "2.0.5",
    "jwt-decode": "4.0.0",
    "keycloak-js": "24.0.1",
    "loglevel": "1.9.1",
    "loglevel-plugin-prefix": "0.8.4",
    "pinia": "2.1.7",
    "vue": "3.4.21",
    "vue-i18n": "9.10.1",
    "vue-router": "4.3.0",
    "vuetify": "3.5.9"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "3.0.1",
    "@rushstack/eslint-patch": "1.7.2",
